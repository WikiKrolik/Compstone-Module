<?xml version="1.0" encoding="utf-8"?>
    <layout xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="accelerometer" type="com.example.phonelocalization.FirstFragment"/>
        <variable name="gyroscope" type="com.example.phonelocalization.FirstFragment"/>
        <variable name="wifi" type="com.example.phonelocalization.FirstFragment"/>
        <variable name="magnetometer" type="com.example.phonelocalization.FirstFragment"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

        <TextView
            android:id="@+id/accelerometer_text"
            android:layout_width="202dp"
            android:layout_height="36dp"
            android:layout_marginTop="32dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Accelerometer:"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/accelerometer_data"
            android.text="@accelerometer"
            android:layout_width="202dp"
            android:layout_height="88dp"
            android:layout_marginTop="16dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accelerometer_text"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/gyroscope_data"
            android.text="@gyroscope"
            android:layout_width="202dp"
            android:layout_height="88dp"
            android:layout_marginTop="16dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gyroscope_text"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/magnetometer_data"
            android.text="@magnetometer"
            android:layout_width="202dp"
            android:layout_height="88dp"
            android:layout_marginTop="16dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/magnetometer_text"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/wifi_data"
            android.text="@wifi"
            android:layout_width="197dp"
            android:layout_height="49dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/wifi_text"
            app:layout_constraintVertical_bias="0.034"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/gyroscope_text"
            android:layout_width="202dp"
            android:layout_height="36dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Gyroscope:"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/wifi_data"
            app:layout_constraintVertical_bias="0.084" />

        <TextView
            android:id="@+id/wifi_text"
            android:layout_width="202dp"
            android:layout_height="36dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Wifi:"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accelerometer_data"
            app:layout_constraintVertical_bias="0.065" />

        <TextView
            android:id="@+id/magnetometer_text"
            android:layout_width="202dp"
            android:layout_height="36dp"
            android:text="Magnetometer:"
            android:fontFamily="serif"
            android:gravity="center"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_marginTop="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gyroscope_data" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>